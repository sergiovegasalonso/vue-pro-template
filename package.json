{
  "name": "vue-pro-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "env NODE_ENV=staging vite --mode staging",
    "dev:staging": "env NODE_ENV=staging vite --mode staging",
    "dev:production": "env NODE_ENV=production vite --mode production",
    "build": "vue-tsc --noEmit && env NODE_ENV=production vite build --mode production",
    "build:staging": "vue-tsc --noEmit && env NODE_ENV=staging vite build --mode staging",
    "build:production": "vue-tsc --noEmit && env NODE_ENV=production vite build --mode production",
    "preview": "vite preview",
    "audit:staging": "yarn audit",
    "audit:production": "yarn audit --groups dependencies"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "pinia": "^2.0.10",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@vitejs/plugin-vue": "^2.0.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.17",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.2.5",
    "vue-tsc": "^0.29.8"
  }
}
